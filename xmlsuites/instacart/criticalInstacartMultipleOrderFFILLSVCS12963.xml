<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Fulfillment ETE">
    <listeners>
        <listener class-name="com.krogerqa.seleniumcentral.framework.listeners.ExtentTestNGIReporterListener"/>
        <listener class-name="com.krogerqa.seleniumcentral.framework.listeners.TestNG_ConsoleRunner"/>
        <listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener"/>
    </listeners>

    <test name="1:Order creation and New order validation in Cue" parallel="methods" thread-count="999">
        <parameter name="grid" value="selenoidGrid"/>
        <parameter name="platform" value="Linux"/>
        <parameter name="browser" value="chrome"/>
        <parameter name="browserVersion" value="latest"/>
        <parameter name="url" value="https://www-stage.kroger.com/signin"/>
        <parameter name="wait" value="20"/>
        <parameter name="logging" value="info"/>
        <parameter name="driverType" value="browser"/>
        <parameter name="highlight" value="false"/>
        <parameter name="retryFailedTests" value="0"/>
        <groups>
            <run>
                <include name="ete_instacart_tc_12963_web_part1_nonEbt"/>
            </run>
        </groups>
        <packages>
            <package name="com.krogerqa.web.cases.instacart"/>
        </packages>
    </test>
    <test name="2: Staging using Harvester Native">
        <parameter name="grid" value="mobileGrid"/>
        <parameter name="mobilePlatformName" value="android"/>
        <parameter name="mobilePlatformVersion" value="13"/>
        <parameter name="driverType" value="appium"/>
        <parameter name="fullReset" value="false"/>
        <parameter name="wait" value="70"/>
        <parameter name="singleSessionSuite" value="false"/>
        <parameter name="artifactId" value="harvester-stage"/>
        <parameter name="artifactVersion" value="${HARVESTER_VERSION}"/>
        <parameter name="artifactGroup" value="com.kroger.harvester"/>
        <parameter name="artifactClassifier" value="release"/>
        <parameter name="androidApkPackage" value="com.android.chrome"/>
        <parameter name="androidApkActivity" value="org.chromium.chrome.browser.firstrun.FirstRunActivity"/>
        <groups>
            <run>
                <include name="ete_instacart_tc_12963_native_part1_nonEbt"/>
            </run>
        </groups>
        <packages>
            <package name="com.krogerqa.mobile.cases.instacart"/>
        </packages>
    </test>
    <test name="3: Staged status validation in Cue" thread-count="999">
        <parameter name="grid" value="selenoidGrid"/>
        <parameter name="platform" value="Linux"/>
        <parameter name="browser" value="chrome"/>
        <parameter name="browserVersion" value="latest"/>
        <parameter name="url" value="https://ecsb-stage.kroger.com/cue/01400413/orders"/>
        <parameter name="wait" value="30"/>
        <parameter name="logging" value="info"/>
        <parameter name="driverType" value="browser"/>
        <parameter name="highlight" value="false"/>
        <parameter name="retryFailedTests" value="0"/>
        <groups>
            <run>
                <include name="ete_instacart_tc_12963_web_part2_nonEbt"/>
            </run>
        </groups>
        <packages>
            <package name="com.krogerqa.web.cases.instacart"/>
        </packages>
    </test>
    <test name="4: Destaging and checkout using Ciao native">
        <parameter name="grid" value="mobileGrid"/>
        <parameter name="mobilePlatformName" value="android"/>
        <parameter name="mobilePlatformVersion" value="13"/>
        <parameter name="driverType" value="appium"/>
        <parameter name="fullReset" value="false"/>
        <parameter name="wait" value="70"/>
        <parameter name="singleSessionSuite" value="false"/>
        <parameter name="artifactGroup" value="com.kroger.ecommpos"/>
        <parameter name="artifactId" value="CiaoAndroid-debug"/>
        <parameter name="artifactVersion" value="${CIAO_VERSION}"/>
        <parameter name="androidApkPackage" value="com.android.chrome"/>
        <parameter name="androidApkActivity" value="org.chromium.chrome.browser.firstrun.FirstRunActivity"/>
        <groups>
            <run>
                <include name="ete_instacart_tc_12963_native_part2_nonEbt"/>
            </run>
        </groups>
        <packages>
            <package name="com.krogerqa.mobile.cases.instacart"/>
        </packages>
    </test>
    <test name="5:Picked up and paid status validation in Cue and Klog validation" parallel="methods" thread-count="999">
        <parameter name="grid" value="selenoidGrid"/>
        <parameter name="platform" value="Linux"/>
        <parameter name="browser" value="chrome"/>
        <parameter name="browserVersion" value="latest"/>
        <parameter name="url" value="https://ecsb-stage.kroger.com/cue/01400413/orders"/>
        <parameter name="wait" value="30"/>
        <parameter name="logging" value="info"/>
        <parameter name="driverType" value="browser"/>
        <parameter name="highlight" value="false"/>
        <parameter name="retryFailedTests" value="0"/>
        <groups>
            <run>
                <include name="ete_instacart_tc_12963_web_part3_nonEbt"/>
            </run>
        </groups>
        <packages>
            <package name="com.krogerqa.web.cases.instacart"/>
        </packages>
    </test>
</suite>