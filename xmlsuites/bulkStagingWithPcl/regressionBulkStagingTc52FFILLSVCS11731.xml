        <parameter name="highlight" value="false"/>
        <parameter name="retryFailedTests" value="0"/>
        <parameter name="singleSessionSuite" value="true"/>
        <groups>
            <run>
                <include name="ete_pcl_tc_11731_bulkStaging_web_part1_nonEbt"/>
            </run>
        </groups>
        <packages>
            <package name="com.krogerqa.web.cases.bulkStagingWithPCL"/>
        </packages>
    </test>

