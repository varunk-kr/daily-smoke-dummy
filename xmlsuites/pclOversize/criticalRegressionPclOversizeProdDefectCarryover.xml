        <parameter name="mobilePlatformName" value="android"/>
        <parameter name="mobilePlatformVersion" value="10"/>
        <parameter name="deviceType" value="emulator"/>
        <parameter name="appiumPort" value="4444"/>
        <parameter name="driverType" value="appium"/>
        <parameter name="fullReset" value="true"/>
        <parameter name="wait" value="80"/>
        <parameter name="androidApkPackage" value="com.android.chrome"/>
        <parameter name="androidApkActivity" value="org.chromium.chrome.browser.firstrun.FirstRunActivity"/>
        <parameter name="androidApkPath" value="${HARVESTER_VERSION}"/>
