<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Fulfillment ETE">
    <listeners>
        <listener class-name="com.krogerqa.seleniumcentral.framework.listeners.ExtentTestNGIReporterListener"/>
        <listener class-name="com.krogerqa.seleniumcentral.framework.listeners.TestNG_ConsoleRunner"/>
        <listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener"/>
    </listeners>

    <test name="Order creation and New order validation in Cue for PCL os prod issue item movement" parallel="methods"
          thread-count="999">
        <parameter name="grid" value="selenoidGrid"/>
        <parameter name="platform" value="Linux"/>
        <parameter name="browser" value="chrome"/>
        <parameter name="browserVersion" value="latest"/>
        <parameter name="url" value="https://www-stage.kingsoopers.com/signin"/>
        <parameter name="wait" value="70"/>
        <parameter name="logging" value="info"/>
        <parameter name="driverType" value="browser"/>
        <parameter name="highlight" value="false"/>
        <parameter name="retryFailedTests" value="0"/>
        <groups>
            <run>
                <include name="ete_byob_pcl_itemMovementToOS_tc_7463_web_part1_nonEbt"/>
            </run>
        </groups>
        <packages>
            <package name="com.krogerqa.web.cases.byob"/>
        </packages>
    </test>
    <test name="Selecting using Harvester Native for pcl Prod defect item movement test">
        <parameter name="grid" value="mobileGrid"/>
        <parameter name="udid" value="emulator-5554"/>
        <parameter name="mobilePlatformName" value="android"/>
        <parameter name="mobilePlatformVersion" value="10"/>
        <parameter name="deviceType" value="emulator"/>
        <parameter name="appiumPort" value="4444"/>
        <parameter name="driverType" value="appium"/>
        <parameter name="fullReset" value="false"/>
        <parameter name="wait" value="150"/>
        <parameter name="singleSessionSuite" value="false"/>
        <parameter name="androidApkPackage" value="com.android.chrome"/>
        <parameter name="androidApkActivity" value="org.chromium.chrome.browser.firstrun.FirstRunActivity"/>
        <parameter name="androidApkPath" value="${HARVESTER_VERSION}"/>
        <groups>
            <run>
                <include name="ete_byob_pcl_itemMovementToOS_tc_7463_native_part1_nonEbt"/>
            </run>
        </groups>
        <packages>
            <package name="com.krogerqa.mobile.cases.byob"/>
        </packages>
    </test>

    <test name="Picking status validation in Cue for item movement scenario"  thread-count="999">
        <parameter name="grid" value="selenoidGrid"/>
        <parameter name="platform" value="Linux"/>
        <parameter name="browser" value="chrome"/>
        <parameter name="browserVersion" value="latest"/>
        <parameter name="url" value="https://ecsb-stage.kroger.com/cue/01400413/orders"/>
        <parameter name="wait" value="50"/>
        <parameter name="logging" value="info"/>
        <parameter name="driverType" value="browser"/>
        <parameter name="highlight" value="false"/>
        <parameter name="retryFailedTests" value="0"/>
        <groups>
            <run>
                <include name="ete_byob_pcl_itemMovementToOS_tc_7463_web_part2_nonEbt"/>
            </run>
        </groups>
        <packages>
            <package name="com.krogerqa.web.cases.byob"/>
        </packages>
    </test>

    <test name="Staging using Harvester Native for pcl os bug">
        <parameter name="grid" value="mobileGrid"/>
        <parameter name="udid" value="emulator-5554"/>
        <parameter name="mobilePlatformName" value="android"/>
        <parameter name="mobilePlatformVersion" value="10"/>
        <parameter name="deviceType" value="emulator"/>
        <parameter name="appiumPort" value="4444"/>
        <parameter name="driverType" value="appium"/>
        <parameter name="fullReset" value="false"/>
        <parameter name="wait" value="60"/>
        <parameter name="singleSessionSuite" value="false"/>
        <parameter name="androidApkPackage" value="com.android.chrome"/>
        <parameter name="androidApkActivity" value="org.chromium.chrome.browser.firstrun.FirstRunActivity"/>
        <parameter name="androidApkPath" value="${HARVESTER_VERSION}"/>
        <groups>
            <run>
                <include name="ete_byob_pcl_itemMovementToOS_tc_7463_native_part2_nonEbt"/>
            </run>
        </groups>
        <packages>
            <package name="com.krogerqa.mobile.cases.byob"/>
        </packages>
    </test>
</suite>