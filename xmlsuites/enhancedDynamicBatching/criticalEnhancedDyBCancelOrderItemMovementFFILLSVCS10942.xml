<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Regression ETE" parallel="false" preserve-order="true" thread-count="1" group-by-instances="true">
    <listeners>
        <listener
                class-name="com.krogerqa.seleniumcentral.framework.listeners.ExtentTestNGIReporterListener"/>
        <listener class-name="com.krogerqa.seleniumcentral.framework.listeners.TestNG_ConsoleRunner"/>
        <listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener"/>
    </listeners>

    <test name="1: Order creation and New order validation in Cue for cancel order item movement"  
          thread-count="999">
        <parameter name="grid" value="selenoidGrid"/>
        <parameter name="platform" value="Linux"/>
        <parameter name="browser" value="chrome"/>
        <parameter name="browserVersion" value="latest"/>
        <parameter name="url" value="https://www-stage.kroger.com/signin"/>
        <parameter name="wait" value="150"/>
        <parameter name="logging" value="info"/>
        <parameter name="driverType" value="browser"/>
        <parameter name="highlight" value="false"/>
        <parameter name="retryFailedTests" value="0"/>
        <parameter name="singleSessionSuite" value="true"/>
        <groups>
            <run>
                <include name="ete_db_enhancedBatchPcl_cancel_Order_ItemMovement_web_part1"/>
            </run>
        </groups>
        <packages>
            <package name="com.krogerqa.web.cases.enhancedDynamicBatching"/>
        </packages>
    </test>

    <test name="2: Selecting using Harvester Native for cancel order item movement">
        <parameter name="grid" value="mobileGrid"/>
        <parameter name="udid" value="emulator-5554"/>
        <parameter name="mobilePlatformName" value="android"/>
        <parameter name="mobilePlatformVersion" value="10"/>
        <parameter name="deviceType" value="emulator"/>
        <parameter name="appiumPort" value="4444"/>
        <parameter name="driverType" value="appium"/>
        <parameter name="fullReset" value="false"/>
        <parameter name="wait" value="150"/>
        <parameter name="singleSessionSuite" value="false"/>
        <parameter name="androidApkPackage" value="com.android.chrome"/>
        <parameter name="androidApkActivity" value="org.chromium.chrome.browser.firstrun.FirstRunActivity"/>
        <parameter name="androidApkPath" value="/builds/harvester-stage-3.83.1-debug.apk"/>
        <groups>
            <run>
                <include name="ete_db_enhancedBatchPcl_cancel_Order_ItemMovement_native_part1"/>
            </run>
        </groups>
        <packages>
            <package name="com.krogerqa.mobile.cases.enhancedDynamicBatching"/>
